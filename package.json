    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@types/core-js": "^0.9.43",
    "@types/jasmine": "^2.6.2",
    "@types/node": "^8.0.47",
    "awesome-typescript-loader": "^3.3.0",
    "codelyzer": "^3.2.2",
    "commitizen": "^2.9.6",
    "concurrently": "^3.5.0",
    "core-js": "^2.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lint-staged": "^4.3.0",
    "ng-packagr": "^1.5.0-rc.1",
    "prettier": "^1.8.2",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "^5.5.2",
    "semantic-release": "^8.2.3",
    "source-map-loader": "^0.2.3",
    "systemjs": "^0.20.19",
    "ts-helpers": "^1.1.1",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.4.0",
    "webpack": "^3.8.1",
    "zone.js": "^0.8.18"
  },
  "bugs": {
    "url": "http://github.com/Cloudtasks/ngx-image/issues"
  },
  "scripts": {
    "rimraf": "rimraf",
    "lint": "tslint ./src/{,*/}*.ts",
    "pretest": "npm run rimraf -- dist coverage",
    "test": "karma start",
    "test-watch": "karma start --singleRun=false --autoWatch=true",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist coverage",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm i",
    "commit": "git-cz",
    "prebuild": "npm run rimraf -- dist",
    "build": "ng-packagr -p ng-package.json",
    "semantic-release":
      "semantic-release pre && npm run build && npm publish dist && semantic-release post",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "precommit": "lint-staged"
  },
  "main": "dist/bundles/ngx-image.umd.js",
  "module": "dist/@cloudtasks/ngx-image.es5.js",
  "typings": "dist/ngx-image.d.ts",
  "es2015": "dist/@cloudtasks/ngx-image.js",
  "metadata": "dist/ngx-image.metadata.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudtasks/ngx-image.git"
  },
  "keywords": [
    "angular",
    "AngularJS",
    "cloudtasks",
    "image",
    "images",
    "processing",
    "image-processing",
    "optimisation",
    "image-optimisation"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6.9.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,json}": ["prettier --write", "git add"]
  },
  "version": "0.0.0-development"
}
